#include <iostream>
#include <conio.h>

using namespace std;
int main()
{
	int first, sec, third;

	cout << "Enter first number: "; cin >> first;
	cout << "Enter second number: "; cin >> sec;
	cout << "Enter third number: "; cin >> third;

	if (first > sec && first > third)
		cout << "The LARGEST number is: " << first;
	else if (sec > first && sec > third)
		cout << "The LARGEST number is: " << sec;
	else if (third > first && third > sec)
		cout << "The LARGEST number is: " << third;
	else
		cout << "There is no LARGEST number; the numbers are all equal.";

	_getch();
	return 0;
}
